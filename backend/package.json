    "pg": "^8.16.3",
    "pg-copy-streams": "^7.0.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/pg": "^8.15.6",
    "@types/pg-copy-streams": "^1.2.5",
    "prisma": "^5.14.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
