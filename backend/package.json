{
  "name": "bundestagswahl-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "load-csv": "ts-node src/loadCsvData.ts",
    "verify": "ts-node src/verifyData.ts",
    "generate-ballots": "ts-node src/generateBallots.ts",
    "verify-ballots": "ts-node src/verifyBallots.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@types/node": "^24.10.0",
    "csv-parse": "^6.1.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "prisma": "^5.14.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
